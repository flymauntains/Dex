        // address pair = pairFor(factory, tokenA, tokenB);
        // console.log("fly_getReserves_pair_address", pair);

        // // Create an instance of the IUniswapV2Pair interface using the pair contract address
        // IUniswapV2Pair pairContract = IUniswapV2Pair(pair);
        // console.log( pairContract);
        // Call getReserves on the pair contract instance to retrieve reserves
        // (uint256 reserve0, uint256 reserve1, ) = IUniswapV2Pair(pairContract).getReserves();
        // (uint112 reserve0, uint112 reserve1, ) = pairContract.getReserves();
        // console.log("fly_getReserves_reserves", reserve0, reserve1);

        // Call getReserves on the pair contract instance to retrieve reserves
        // (uint reserve0, uint reserve1, ) = pairContract.getReserves();
        // console.log("fly_getReserves_reserves", reserve0, reserve1);
            console.log("fly_UniswapV2Library_getReserves_token0", token0);
            console.log("fly_UniswapV2Library_getReserves_token0_pairFor", pairFor(factory, tokenA, tokenB));
            (uint reserve0, uint reserve1, ) = IUniswapV2Pair(pairFor(factory, tokenA, tokenB)).getReserves();
